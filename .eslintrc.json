{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true,
        "amd": true,
        "jquery": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": 2021,
        "sourceType": "module",
        "ecmaFeatures": {
            "topLevelAwait": true
        }
    },
    "rules": {
        "no-console": "off",
        "no-unused-vars": ["warn", { "argsIgnorePattern": "next" }],
        "no-prototype-builtins": "off",
        "no-extra-semi": "error",
        "import/no-unused-modules": "off",
        "unicorn/better-regex": "off",
        "no-self-assign": "off",
        "no-undef": "off"
    },
    "ignorePatterns": [
        "**/*.min.js",
        "src/public/assets/js/bootstrap/bootstrap.esm.min.js",
        "src/public/assets/js/bootstrap/bootstrap.bundle.js",
        "src/public/assets/js/bootstrap/bootstrap.esm.js",
        "src/public/assets/js/bootstrap/bootstrap.js"
    ]
}
